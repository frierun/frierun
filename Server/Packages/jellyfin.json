{
  "Name": "jellyfin",
  "Url": "https://jellyfin.org/docs/general/installation/container",
  "Contracts": [
    {
      "Type": "Container",
      "ImageName": "jellyfin/jellyfin:latest"
    },
    {
      "Type": "HttpEndpoint",
      "Port": 8096
    },
    {
      "Type": "Mount",
      "VolumeName": "config",
      "Path": "/config"
    },
    {
      "Type": "Mount",
      "VolumeName": "cache",
      "Path": "/cache"
    },
    {
      "Type": "Mount",
      "VolumeName": "media",
      "Path": "/media",
      "ReadOnly": true
    }
  ]
}