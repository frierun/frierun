{
  "Name": "prowlarr",
  "Url": "https://docs.linuxserver.io/images/docker-prowlarr/",
  "Contracts": [
    {
      "Type": "Container",
      "ImageName": "lscr.io/linuxserver/prowlarr:latest",
      "Env": {
        "PUID": "1000",
        "PGID": "1000",
        "TZ": "Etc/UTC"
      }
    },
    {
      "Type": "HttpEndpoint",
      "Port": 9696
    },
    {
      "Type": "Mount",
      "VolumeName": "config",
      "Path": "/config"
    }
  ]
}