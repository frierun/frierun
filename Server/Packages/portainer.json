{
  "Name": "portainer",
  "Url": "https://docs.portainer.io/start/install-ce/server/docker/linux",
  "Children": [
    {
      "Type": "Container",
      "ImageName": "portainer/portainer-ce:latest",
      "RequireDocker": true,
      "Children": [
        {
          "Type": "HttpEndpoint",
          "Port": 9000
        },
        {
          "Type": "Volume",
          "Name": "data",
          "Path": "/data"
        }
      ]
    }
  ]
}