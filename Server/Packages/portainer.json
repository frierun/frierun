{
  "Name": "portainer",
  "Url": "https://docs.portainer.io/start/install-ce/server/docker/linux",
  "Contracts": [
    {
      "Type": "Container",
      "ImageName": "portainer/portainer-ce:latest",
      "RequireDocker": true
    },
    {
      "Type": "HttpEndpoint",
      "Port": 9000
    },
    {
      "Type": "Mount",
      "VolumeName": "data",
      "Path": "/data"
    }
  ]
}