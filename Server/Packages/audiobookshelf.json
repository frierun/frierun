{
  "Name": "audiobookshelf",
  "Url": "https://www.audiobookshelf.org/docs",
  "Contracts": [
    {
      "Type": "Container",
      "ImageName": "ghcr.io/advplyr/audiobookshelf:latest"
    },
    {
      "Type": "HttpEndpoint",
      "Port": 80
    },
    {
      "Type": "Mount",
      "VolumeName": "audiobooks",
      "Path": "/audiobooks"
    },
    {
      "Type": "Mount",
      "VolumeName": "podcasts",
      "Path": "/podcasts"
    },
    {
      "Type": "Mount",
      "VolumeName": "config",
      "Path": "/config"
    },
    {
      "Type": "Mount",
      "VolumeName": "metadata",
      "Path": "/metadata"
    }
  ]
}