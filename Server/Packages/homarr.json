{
  "Name": "homarr",
  "Url": "https://homarr.dev/docs/getting-started/installation/",
  "Contracts": [
    {
      "Type": "Container",
      "ImageName": "ghcr.io/ajnart/homarr:latest",
      "RequireDocker": true
    },
    {
      "Type": "HttpEndpoint",
      "Port": 7575
    },
    {
      "Type": "Mount",
      "VolumeName": "configs",
      "Path": "/app/data/configs"
    },
    {
      "Type": "Mount",
      "VolumeName": "icons",
      "Path": "/app/public/icons"
    },
    {
      "Type": "Mount",
      "VolumeName": "data",
      "Path": "/data"
    }
  ]
}