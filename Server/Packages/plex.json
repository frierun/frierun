{
  "Name": "plex",
  "Url": "https://github.com/plexinc/pms-docker/",
  "Contracts": [
    {
      "Type": "Container",
      "ImageName": "plexinc/pms-docker:latest",
      "Env": {
        "PLEX_CLAIM": "{{Parameter:PlexClaim:Value}}"
      }
    },
    {
      "Type": "HttpEndpoint",
      "Port": 32400
    },
    {
      "Type": "PortEndpoint",
      "Port": 8324,
      "Protocol": "tcp"
    },
    {
      "Type": "PortEndpoint",
      "Port": 32469,
      "Protocol": "tcp"
    },
    {
      "Type": "PortEndpoint",
      "Port": 1900,
      "Protocol": "udp"
    },
    {
      "Type": "PortEndpoint",
      "Port": 32410,
      "Protocol": "udp"
    },
    {
      "Type": "PortEndpoint",
      "Port": 32412,
      "Protocol": "udp"
    },
    {
      "Type": "PortEndpoint",
      "Port": 32413,
      "Protocol": "udp"
    },
    {
      "Type": "PortEndpoint",
      "Port": 32414,
      "Protocol": "udp"
    },
    {
      "Type": "Mount",
      "VolumeName": "database",
      "Path": "/config"
    },
    {
      "Type": "Mount",
      "VolumeName": "transcode",
      "Path": "/transcode"
    },
    {
      "Type": "Mount",
      "VolumeName": "media",
      "Path": "/data"
    }
  ]
}