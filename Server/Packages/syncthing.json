{
  "Name": "syncthing",
  "Url": "https://github.com/linuxserver/docker-syncthing",
  "Contracts": [
    {
      "Type": "File",
      "VolumeName": "custom-init",
      "Path": "chown.sh",
      "Text": "#!/bin/bash\n\necho \"**** chowning /data ****\"\nchown $PUID:$PGID /data\n"
    },
    {
      "Type": "Container",
      "ImageName": "lscr.io/linuxserver/syncthing:latest",
      "Env": {
        "PUID": "1000",
        "PGID": "1000",
        "TZ": "Etc/UTC"
      }      
    },
    {
      "Type": "HttpEndpoint",
      "Port": 8384
    },
    {
      "Type": "PortEndpoint",
      "Port": 22000,
      "Protocol": "tcp"
    },
    {
      "Type": "PortEndpoint",
      "Port": 22000,
      "Protocol": "udp"
    },
    {
      "Type": "PortEndpoint",
      "Port": 21027,
      "Protocol": "udp"
    },
    {
      "Type": "Mount",
      "VolumeName": "data",
      "Path": "/data"
    },
    {
      "Type": "Mount",
      "VolumeName": "config",
      "Path": "/config"
    },
    {
      "Type": "Mount",
      "VolumeName": "custom-init",
      "Path": "/custom-cont-init.d",
      "ReadOnly": true
    }
  ]
}