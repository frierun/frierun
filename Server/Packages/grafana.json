{
  "Name": "grafana",
  "Url": "https://grafana.com/docs/grafana/latest/setup-grafana/installation/docker/",
  "ApplicationDescription": "Default login: admin\nDefault password: admin",
  "Contracts": [
    {
      "Type": "Container",
      "ImageName": "grafana/grafana-oss:latest",
      "Env": {
        "LOG_LEVEL": "debug",
        "TZ": "Etc/UTC"
      }
    },
    {
      "Type": "HttpEndpoint",
      "Port": 3000
    },
    {
      "Type": "Mount",
      "VolumeName": "config",
      "Path": "/var/lib/grafana"
    }
  ]
}