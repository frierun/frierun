{
  "Name": "sonarr",
  "Url": "https://docs.linuxserver.io/images/docker-sonarr/",
  "Contracts": [
    {
      "Type": "File",
      "VolumeName": "custom-init",
      "Path": "chown.sh",
      "Text": "#!/bin/bash\n\necho \"**** chowning /data ****\"\nchown $PUID:$PGID /data\n"
    },
    {
      "Type": "Container",
      "ImageName": "lscr.io/linuxserver/sonarr:latest",
      "Env": {
        "PUID": "1000",
        "PGID": "1000",
        "TZ": "Etc/UTC"
      }
    },
    {
      "Type": "HttpEndpoint",
      "Port": 8989
    },
    {
      "Type": "Mount",
      "VolumeName": "config",
      "Path": "/config"
    },
    {
      "Type": "Mount",
      "VolumeName": "data",
      "Path": "/data"
    },
    {
      "Type": "Mount",
      "VolumeName": "custom-init",
      "Path": "/custom-cont-init.d"
    }
  ]
}