{
  "Name": "mealie",
  "Url": "https://github.com/FreshRSS/FreshRSS/tree/edge/Docker",
  "Contracts": [
    {
      "Type": "Container",
      "ImageName": "ghcr.io/mealie-recipes/mealie:latest",
      "Env": {
        "ALLOW_SIGNUP": "false",
        "PUID": "1000",
        "PGID": "1000",
        "TZ": "Etc/UTC"
      }
    },
    {
      "Type": "HttpEndpoint",
      "Port": 9000
    },
    {
      "Type": "Mount",
      "VolumeName": "data",
      "Path": "/app/data"
    }
  ]
}